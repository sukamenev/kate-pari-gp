        <!-- Contexts -->
        <contexts>
            <context name="Normal" attribute="Normal Text" lineEndContext="#stay">
                <keyword attribute="Keyword" context="#stay" String="keywords"/>
                <keyword attribute="Function" context="#stay" String="functions"/>
                <keyword attribute="Constant" context="#stay" String="constants"/>
                <keyword attribute="Flow of control" context="#stay" String="flowofcontrol"/>
                
                <!-- Numbers -->
                <RegExpr attribute="Number" context="#stay" String="\b[0-9]+(\.[0-9]+)?([eE][+-]?[0-9]+)?\b"/>
                
                <!-- Strings -->
                <DetectChar attribute="String" context="String" char="&quot;"/>
                
                <!-- Comments -->
                <RegExpr attribute="Comment" context="#stay" String="\\\\[^\n]*$"/>
                <StringDetect attribute="Comment" context="MultiLineComment" String="/*"/>

                <!-- Brackets -->
                <DetectChar attribute="Brace" context="#stay" char="{"/>
                <DetectChar attribute="Brace" context="#stay" char="}"/>
                <DetectChar attribute="Parenthesis" context="#stay" char="("/>
                <DetectChar attribute="Parenthesis" context="#stay" char=")"/>
                <DetectChar attribute="Bracket" context="#stay" char="["/>
                <DetectChar attribute="Bracket" context="#stay" char="]"/>

                <!-- PARI/GP service commands (starting with \) -->
                <RegExpr attribute="Operator" context="#stay" String="\\(a|B|c|d|e|g|gf|gm|h|l|m|o|p|pb|ps|q|r|s|t|u|um|uv|v|w|x|y)\s"/>
                
                <!-- Operators -->
                <RegExpr attribute="Operator" context="#stay" String="(\+=|%=|==|!=|&lt;=|&gt;=|&amp;&amp;|\|\||\.\.|-&gt;|&lt;&lt;|&gt;&gt;|\+\+|--)"/>
                <RegExpr attribute="Operator" context="#stay" String="(\+|-|\*|/|\\|\^|%|=|&lt;|&gt;|!|~|\||&amp;)"/>
            </context>

            <context name="String" attribute="String" lineEndContext="#pop">
                <DetectChar attribute="String" context="#pop" char="&quot;"/>
                <RegExpr attribute="String" context="#stay" String="\\."/>
            </context>
            
            <context name="MultiLineComment" attribute="Comment" lineEndContext="#stay">
                <RegExpr attribute="Comment" context="#stay" String=".*?(?=\*/)"/>
                <StringDetect attribute="Comment" context="#pop" String="*/" endRegion="comment"/>
            </context>
        </contexts>

        <!-- Styles -->
